<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test name="ChromeTest" parallel="tests" thread-count="2">
    <parameter name="browser" value="chrome"/>
    <classes>
      <class name="testscripts.lab3"/>
      <class name="testscripts.lab4"/>
    </classes>
  </test>

  <test name="FirefoxTest" parallel="tests" thread-count="2">
    <parameter name="browser" value="firefox"/>
    <classes>
      <class name="testscripts.lab3"/>
      <class name="testscripts.lab4"/>
    </classes>
  </test>

  <test name="EdgeTest" parallel="tests" thread-count="2">
    <parameter name="browser" value="edge"/>
    <classes>
      <class name="testscripts.lab3"/>
      <class name="testscripts.lab4"/>
    </classes>
  </test>
</suite>
